core.ignore.recursive= \
	# Built application files
	{*.apk} \
	{*.ap_} \

	# Files for the Dalvik VM
	{*.dex} \

	# Java class files
	{*.class} \

	# Log Files
	{*.log} \

	# Gradle Files
	{build}

core.ignore= \
	# Generated files
	{bin} \
	{gen} \

	# Gradle files
	{.gradle} \
	{build} \
	
	# Local configuration file (sdk path, etc)
	{local.properties} \

	# Proguard folder generated by Eclipse
	{proguard}

